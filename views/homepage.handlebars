{{log this}}
{{#each listData as |List| }}
<div class="column">
  <div class="col-auto">
    <h4>{{List.title}}</h4>
  </div>
</div>
{{#each List.bucketListItems as |Item| }}
<div class="row mt-4">
  <div class="col-md-6">
    <div class="container">
      
        <div id="BucketList">
          <h4>{{List.title}}</h4>
                {{#each List.bucketListItems as |item| }}                
                <button type ="travel-lock" class="btn btn-primary">Travel</button>
                <ul id="travel-list"></ul>
                {{#if (ifCond item.category "travel")}}
                <li>{{item.body}}</li>
                {{/if}}
                <br>
                <button type ="travel-lock" class="btn btn-primary">Food</button>
                <ul id ="food-list"></ul>
                {{#if (ifCond item.category "food")}}
                <li>{{item.body}}</li>
                {{/if}}
                <br> 
                <button type ="travel-lock" class="btn btn-primary">Adrenaline</button>
                <ul id ="adrenaline-list"></ul>
                {{#if (ifCond item.category "adrenaline")}}
                <li>{{item.body}}</li>
                <br>
                {{/if}}
                {{/each}}                
        <div>
      {{/each}}
      {{/each}}

        <button type="submit" class="btn btn-primary">Clear</button>
         <button type="save" class="btn btn-primary">Save</button>
          <button type="email" class="btn btn-primary">Email</button>
      </div>
      </div>
    </div>
      <br>
      <div id="container">
        <br>
        <button type ="generate-lock" class="btn btn-primary" id="generate">Generate</button>
                <ul id="travel-list"></ul>
      </div>
  </div>

 

<script src="./js/homepage.js"></script>
